{
    "name": "yd/ypf",
    "description": "YunDun php framework",
    "homepage": "http://www.yundun.com",
    "keywords": ["php"],
    "type": "library",
    "authors": [
        {
            "name": "jasong",
            "email": "gaolu@yundun.com",
            "role": "Development"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/yaml": "v3.4.28"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "friendsofphp/php-cs-fixer": "^2.10|^2.2",
        "monolog/monolog": "1.22.0"
    },
    "autoload": {
        "psr-4": {
            "Ypf\\": "src/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "process-timeout":      600,
        "preferred-install":    "dist",
        "github-protocols":     ["https"]
    }
}
